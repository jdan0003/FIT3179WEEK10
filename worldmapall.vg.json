{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "width": 1500,
    "height": 600,
    "projection": {
        "type": "equalEarth"
    },
    "layer": [
        {
            "data": {
                "url": "https://raw.githubusercontent.com/jdan0003/lab19/main/custom.geo.topo.json",
                "format": {
                    "type": "topojson",
                    "feature": "custom.geo"
                }
            },
            "mark": {
                "type": "geoshape",
                "fill": "lightgrey",
                "stroke": "white"
            }
        },
        {
            "data": {
                "url": "https://raw.githubusercontent.com/jdan0003/lab19/main/The%20Worlds%2050%20Best%20Restaurants%202018.csv",
            },
            "mark": {
                "type": "circle",
                "stroke": "black",
                "fill" : "#6baed6",
                "strokeWidth": 0.75
            },
            "encoding": {
                "longitude": {
                    "field": "Longitude",
                    "type": "quantitative"
                },
                "latitude": {
                    "field": "Latitude",
                    "type": "quantitative"
                },
                "size": {
                    "field": "Ranking",
                    "type": "quantitative",
                    "scale": {
                        "type": "threshold",
                        "domain": [
                            10,
                            20,
                            30,
                            40
                        ],
                        "range": [
                            60,
                            150,
                            220,
                            350,
                            600
                        ]
                    },
                    "legend": {
                        "format": ".1s"
                    },
                    "sort": "descending"
                },
                "tooltip": [
                    {
                        "field": "Ranking",
                        "type": "quantitative"
                    },
                    {
                        "field": "Year",
                        "type": "ordinal"
                    },
                    {
                        "field": "Name",
                        "type": "nominal",
                        "title":"Restaurant"
                    },
                    {
                        "field": "City",
                        "type": "nominal"
                    },
                    {
                        "field": "Latitude",
                        "type": "quantitative"
                    },
                    {
                        "field": "Longitude",
                        "type": "quantitative"
                    },
                    {
                        "field": "Stars",
                        "type": "quantitative",
                        "title": "Michelin Stars"
                    },
                    {
                        "field": "Chef",
                        "type": "nominal"
                    },
                    {
                        "field": "Website",
                        "type": "nominal"
                    },
                    {
                        "field": "Menu",
                        "type": "quantitative",
                        "title": "Price"
                    }
                ]
            }
        },
        {
            "data": {
                "url": "https://raw.githubusercontent.com/jdan0003/lab19/dd22e27311bb2d2b9b021818b4c710b121be5645/50%20best%20restaurants%20database.csv"
            },
            "mark": {
                "type": "circle",
                "stroke": "black",
                "strokeWidth": 0.75,
                "fill":"#fd8d3c"
            },
            "encoding": {
                "longitude": {
                    "field": "Longitude",
                    "type": "quantitative"
                },
                "latitude": {
                    "field": "Latitude",
                    "type": "quantitative"
                },
                "size": {
                    "field": "Ranking",
                    "type": "quantitative",
                    "scale": {
                        "type": "threshold",
                        "domain": [
                            10,
                            20,
                            30,
                            40
                        ],
                        "range": [
                            60,
                            150,
                            220,
                            350,
                            600
                        ]
                    },
                    "legend": {
                        "format": ".1s"
                    },
                    "sort": "descending"
                },
                "color": {
                    "field": "Ranking",
                    "type": "quantitative",
                    "scale": {
                        "domain": [
                            10,
                            20,
                            30,
                            40
                        ],
                        "range": [
                            "#feedde",
                            "#fdbe85",
                            ,
                            "#e6550d",
                            "#a63603"
                        ]
                    },
                    "sort": "descending"
                },
                "tooltip": [
                    {
                        "field": "Ranking",
                        "type": "quantitative"
                    },
                    {
                        "field": "Year",
                        "type": "ordinal"
                    },
                    {
                        "field": "Restaurant",
                        "type": "nominal"
                    },
                    {
                        "field": "City",
                        "type": "nominal"
                    },
                    {
                        "field": "Latitude",
                        "type": "quantitative"
                    },
                    {
                        "field": "Longitude",
                        "type": "quantitative"
                    },
                    {
                        "field": "Stars",
                        "type": "quantitative",
                        "title": "Michelin Stars"
                    },
                    {
                        "field": "Chef",
                        "type": "nominal"
                    },
                    {
                        "field": "Website",
                        "type": "nominal"
                    },
                    {
                        "field": "Menu",
                        "type": "quantitative",
                        "title": "Price"
                    }
                ]
            }
        },
        {
            "data": {
                "url": "https://raw.githubusercontent.com/jdan0003/lab19/main/one-star-michelin-restaurants.csv"
            },
            "mark": {
                "type": "circle",
                "fillOpacity": 0.5,
                "fill": "black",
                "tooltip": {
                    "content": "data"
                }
            },
            "encoding": {
                "longitude": {
                    "field": "longitude",
                    "type": "quantitative"
                },
                "latitude": {
                    "field": "latitude",
                    "type": "quantitative"
                },
                "size": {
                    "value": 10
                }
            }
        },
        {
            "data": {
                "url": "https://raw.githubusercontent.com/jdan0003/lab19/main/two-stars-michelin-restaurants.csv"
            },
            "mark": {
                "type": "circle",
                "fillOpacity": 0.5,
                "fill": "black",
                "tooltip": {
                    "content": "data"
                }
            },
            "encoding": {
                "longitude": {
                    "field": "longitude",
                    "type": "quantitative"
                },
                "latitude": {
                    "field": "latitude",
                    "type": "quantitative"
                },
                "size": {
                    "value": 10
                }
            }
        },
        {
            "data": {
                "url": "https://raw.githubusercontent.com/jdan0003/lab19/main/three-stars-michelin-restaurants.csv"
            },
            "mark": {
                "type": "circle",
                "fillOpacity": 0.5,
                "fill": "black",
                "tooltip": {
                    "content": "data"
                }
            },
            "encoding": {
                "longitude": {
                    "field": "longitude",
                    "type": "quantitative"
                },
                "latitude": {
                    "field": "latitude",
                    "type": "quantitative"
                },
                "size": {
                    "value": 10
                }
            }
        }
    ]
}